<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@color/white"
    android:orientation="vertical">

    <ScrollView
        android:id="@+id/scoll"
        android:layout_width="match_parent"
        android:layout_height="250dp">

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:orientation="vertical">


            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_marginTop="10dp"
                android:gravity="center"
                android:orientation="horizontal">

                <RelativeLayout
                    android:id="@+id/layout_preview"
                    android:layout_width="136dp"
                    android:layout_height="wrap_content"
                    android:paddingBottom="0dp"
                    android:paddingTop="0dp">

                    <LinearLayout
                        android:id="@+id/Llayout_preview"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:gravity="center"
                        android:orientation="vertical"
                        android:visibility="visible">

                        <FrameLayout
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content">

                            <TextureView
                                android:id="@+id/sf_preview"
                                android:layout_width="136dp"
                                android:layout_height="102dp"
                                android:layout_weight="1"
                                android:background="@drawable/grayc"
                                android:visibility="visible" />

                            <ImageView
                                android:id="@+id/back"
                                android:layout_width="136dp"
                                android:layout_height="102dp"
                                android:src="@drawable/forehead" />
                        </FrameLayout>

                        <LinearLayout
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:gravity="center"
                            android:orientation="horizontal">


                            <ImageView
                                android:layout_width="10dp"
                                android:layout_height="10dp"
                                android:layout_marginRight="5dp"
                                android:src="@drawable/green" />

                            <TextView
                                android:layout_width="wrap_content"
                                android:layout_height="wrap_content"
                                android:text="@string/forehead"
                                android:textColor="#000"
                                android:textSize="12dp" />


                        </LinearLayout>


                    </LinearLayout>

                </RelativeLayout>

                <RelativeLayout
                    android:id="@+id/layout_preview2"
                    android:layout_width="136dp"
                    android:layout_height="wrap_content"
                    android:layout_marginLeft="18dp"
                    android:paddingBottom="0dp"
                    android:paddingTop="0dp">

                    <LinearLayout
                        android:id="@+id/Llayout_preview2"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:gravity="center"
                        android:orientation="vertical"
                        android:visibility="visible">

                        <FrameLayout
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content">

                            <TextureView
                                android:id="@+id/sf_preview2"
                                android:layout_width="136dp"
                                android:layout_height="102dp"
                                android:layout_weight="1"
                                android:background="@drawable/grayc"
                                android:visibility="visible" />

                            <ImageView
                                android:id="@+id/back1"
                                android:layout_width="136dp"
                                android:layout_height="102dp"
                                android:src="@drawable/cheek" />

                        </FrameLayout>


                        <LinearLayout
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:gravity="center"
                            android:orientation="horizontal">


                            <ImageView
                                android:layout_width="10dp"
                                android:layout_height="10dp"
                                android:layout_marginRight="5dp"
                                android:src="@drawable/green" />

                            <TextView
                                android:layout_width="wrap_content"
                                android:layout_height="wrap_content"
                                android:text="@string/cheek"
                                android:textColor="#000"
                                android:textSize="12dp" />
                        </LinearLayout>
                    </LinearLayout>
                </RelativeLayout>
            </LinearLayout>


            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_marginTop="8dp"
                android:gravity="center"
                android:orientation="horizontal">

                <RelativeLayout
                    android:id="@+id/layout_preview3"
                    android:layout_width="136dp"
                    android:layout_height="wrap_content"
                    android:paddingBottom="0dp"
                    android:paddingTop="0dp">

                    <LinearLayout
                        android:id="@+id/Llayout_preview3"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:gravity="center"
                        android:orientation="vertical"
                        android:visibility="visible">

                        <FrameLayout
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content">

                            <TextureView
                                android:id="@+id/sf_preview3"
                                android:layout_width="136dp"
                                android:layout_height="102dp"
                                android:layout_weight="1"
                                android:visibility="visible" />

                            <ImageView
                                android:id="@+id/back2"
                                android:layout_width="136dp"
                                android:layout_height="102dp"
                                android:src="@drawable/canthus" />
                        </FrameLayout>


                        <LinearLayout
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:gravity="center"
                            android:orientation="horizontal">


                            <ImageView
                                android:layout_width="10dp"
                                android:layout_height="10dp"
                                android:layout_marginRight="5dp"
                                android:src="@drawable/green" />

                            <TextView
                                android:layout_width="wrap_content"
                                android:layout_height="wrap_content"
                                android:text="@string/eyscorner"
                                android:textColor="#000"
                                android:textSize="12dp" />
                        </LinearLayout>

                    </LinearLayout>

                </RelativeLayout>

                <RelativeLayout
                    android:id="@+id/layout_preview4"
                    android:layout_width="136dp"
                    android:layout_height="wrap_content"
                    android:layout_marginLeft="18dp"
                    android:gravity="center"
                    android:paddingBottom="0dp"
                    android:paddingTop="0dp">

                    <LinearLayout
                        android:id="@+id/Llayout_preview4"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:gravity="center"
                        android:orientation="vertical"
                        android:visibility="visible">

                        <FrameLayout
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content">

                            <TextureView
                                android:id="@+id/sf_preview4"
                                android:layout_width="136dp"
                                android:layout_height="102dp"
                                android:layout_weight="1"
                                android:visibility="visible" />

                            <ImageView
                                android:id="@+id/back3"
                                android:layout_width="136dp"
                                android:layout_height="102dp"
                                android:src="@drawable/pigment" />
                        </FrameLayout>

                        <LinearLayout
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:gravity="center"
                            android:orientation="horizontal">


                            <ImageView
                                android:layout_width="10dp"
                                android:layout_height="10dp"
                                android:layout_marginRight="5dp"
                                android:src="@drawable/red" />

                            <TextView
                                android:layout_width="wrap_content"
                                android:layout_height="wrap_content"
                                android:text="@string/pigment"
                                android:textColor="#000"
                                android:textSize="12dp" />
                        </LinearLayout>
                    </LinearLayout>
                </RelativeLayout>
            </LinearLayout>

            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_marginTop="8dp"
                android:gravity="center"
                android:orientation="horizontal">

                <RelativeLayout
                    android:id="@+id/layout_preview5"
                    android:layout_width="136dp"
                    android:layout_height="wrap_content"
                    android:paddingBottom="0dp"
                    android:paddingTop="0dp">

                    <LinearLayout
                        android:id="@+id/Llayout_preview5"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:gravity="center"
                        android:orientation="vertical"
                        android:visibility="visible">

                        <FrameLayout
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content">

                            <TextureView
                                android:id="@+id/sf_preview5"
                                android:layout_width="136dp"
                                android:layout_height="102dp"
                                android:layout_weight="1"
                                android:visibility="visible" />

                            <ImageView
                                android:id="@+id/back4"
                                android:layout_width="136dp"
                                android:layout_height="102dp"
                                android:src="@drawable/sensitive" />
                        </FrameLayout>

                        <LinearLayout
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:gravity="center"
                            android:orientation="horizontal">

                            <ImageView
                                android:layout_width="10dp"
                                android:layout_height="10dp"
                                android:layout_marginRight="5dp"
                                android:src="@drawable/red" />

                            <TextView
                                android:layout_width="wrap_content"
                                android:layout_height="wrap_content"
                                android:text="@string/sensitive"
                                android:textColor="#000"
                                android:textSize="12dp" />
                        </LinearLayout>
                    </LinearLayout>
                </RelativeLayout>

                <RelativeLayout
                    android:id="@+id/layout_preview6"
                    android:layout_width="136dp"
                    android:layout_height="wrap_content"
                    android:layout_marginLeft="18dp"
                    android:gravity="center"
                    android:paddingBottom="0dp"
                    android:paddingTop="0dp">

                    <LinearLayout
                        android:id="@+id/Llayout_preview6"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:gravity="center"
                        android:orientation="vertical"
                        android:visibility="visible">

                        <FrameLayout
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content">

                            <TextureView
                                android:id="@+id/sf_preview6"
                                android:layout_width="136dp"
                                android:layout_height="102dp"
                                android:layout_weight="1"
                                android:visibility="visible" />

                            <ImageView
                                android:id="@+id/back5"
                                android:layout_width="136dp"
                                android:layout_height="102dp"
                                android:src="@drawable/nosewing" />
                        </FrameLayout>

                        <LinearLayout
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:gravity="center"
                            android:orientation="horizontal">

                            <ImageView
                                android:layout_width="10dp"
                                android:layout_height="10dp"
                                android:layout_marginRight="5dp"
                                android:src="@drawable/blue" />

                            <TextView
                                android:layout_width="wrap_content"
                                android:layout_height="wrap_content"
                                android:text="@string/wing"
                                android:textColor="#000"
                                android:textSize="12dp" />
                        </LinearLayout>
                    </LinearLayout>
                </RelativeLayout>
            </LinearLayout>
        </LinearLayout>
    </ScrollView>

    <RelativeLayout
        android:layout_width="match_parent"
        android:layout_height="0dp"
        android:layout_weight="1">

        <ImageView
            android:id="@+id/zhishi"
            android:layout_width="15dp"
            android:layout_height="15dp"
            android:layout_centerHorizontal="true"
            android:src="@drawable/down" />

        <ImageView
            android:id="@+id/tupina"
            android:layout_width="215dp"
            android:layout_height="215dp"
            android:layout_centerInParent="true"
            android:src="@drawable/etou" />

    </RelativeLayout>

    <LinearLayout
        android:id="@+id/testresult"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_gravity="center"
        android:clickable="true"
        android:gravity="center"
        android:orientation="vertical">

        <ImageView
            android:id="@+id/jiacejieguo"
            android:layout_width="27dp"
            android:layout_height="27dp"
            android:src="@drawable/testersult" />

        <TextView
            android:id="@+id/jiacejieguot"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginBottom="10dp"
            android:text="测试结果"
            android:textSize="9dp" />

    </LinearLayout>

</LinearLayout>